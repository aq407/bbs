<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.demo.bbs.mapper.PublicMapper">

    <insert id = "save">

        INSERT INTO `public` (`id`, `title`, `covers`, `tag`, `user`, `time`, `content`, `synopsis`)
         VALUES (#{id}, #{title}, #{covers}, #{tag}, #{user}, #{time}, #{content}, #{synopsis});
    </insert>


</mapper>